{
	"name": "rambdax",
	"version": "0.8.10",
	"description": "Lightweight alternative to Ramda - extended version",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
	"scripts": {
    "build": "rm -rf dist && tsc --pretty",
    "watch": "tsc --noEmit --pretty --watch",
    "lint": "tslint ./src/**/*.ts",
    "lintx": "tslint ./src/**/*.ts --fix",
    "test": "jest",
    "docs": "docsify init ./docs",
    "de": "commit",
    "do": "node files/do",
    "benchmark": "node benchmarks/index.js --all",
    "cover": "jest __tests__ --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/selfrefactor/rambdax.git"
	},
	"keywords": [
		"rambda",
		"ramda",
    "fp",
    "functional",
    "utility",
		"util"
	],
	"author": "self_refactor",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/selfrefactor/rambdax/issues"
	},
	"homepage": "https://github.com/selfrefactor/rambdax#readme",
	"dependencies": {
	},
	"devDependencies": {
		"@types/jest": "^22.1.2",
		"commit-message": "https://github.com/selfrefactor/commit-message#2.3.2",
		"fs-extra": "^5.0.0",
		"jest": "^22.1.4",
		"rambda": "^1.0.12",
		"rambda-repl": "https://github.com/selfrefactor/rambda-repl#0.3.0",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.3",
    "do": "https://github.com/selfrefactor/do#1.0.0",
    "lodash": "^4.17.4",
    "ramda": "^0.24.1",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
	},
	"jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
